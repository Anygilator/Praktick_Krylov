<Window x:Class="Praktick_Krylov.Registration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praktick_Krylov"
        mc:Ignorable="d"
        Title="Registration" Height="450" Width="800">
    <StackPanel  Height="195" Width="300">
        <Label Content="Логин"/>
        <TextBox Name="TB_Reg_Log"/>
        <Label Content="Пароль"/>
        <TextBox Name="TB_Reg_Pas"/>
        <Label Content="Повторите пароль"/>
        <TextBox Name="TB_Reg_PasAgain"/>
        <Label/>
        <Button Content="Зарегистрироваться" Click="Button_Click_1"/>
        <Label/>
        <Button Content="Есть аккаунт?" Click="Button_Click"/>
    </StackPanel>
</Window>
